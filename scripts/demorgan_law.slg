let not = func(b) => {
    if (b) { false; } else { true; }
};

let and = func(a, b) => { a && b; };
let or = func(a, b) => { a || b; };

let de_morgan_1 = func(a, b) => {
    not(and(a, b)) == or(not(a), not(b));
};

outn(de_morgan_1(true, true));
outn(de_morgan_1(true, false));
outn(de_morgan_1(false, true));
outn(de_morgan_1(false, false));
